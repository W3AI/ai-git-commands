# Firebase Deploy & Resume
echo firebase deploy and resume coding
echo mnemo: Franklin Delano Roosevelt

## nrb - shortcut for npm run build
echo npm run build
npm run build

## fd - shortcut for firebase deploy
echo firebase deploy
# firebase deploy
firebase deploy --except functions

## nrs - shortcut for npm run dev(nuxt)/serve(vue)
echo npm run serve
npm run serve
# echo npm run dev
# npm run dev